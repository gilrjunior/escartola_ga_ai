{
  "buildCommand": "python3 manage.py collectstatic --noinput",
  "builds": [ { "src": "config/wsgi.py", "use": "@vercel/python", "config": { "runtime": "python3.11" } } ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
    { "src": "/(.*)", "dest": "config/wsgi.py" }
  ]
}
